<!-- Special window zoom animation: this is the element that enters the screen, 
	it starts at 200% and scales down. Goes with zoom_exit.xml. -->
<set xmlns:Android="http://schemas.android.com/apk/res/android"
	Android:interpolator="@android:anim/decelerate_interpolator">
	<scale Android:fromXScale="0.9" Android:toXScale="1.0"
		Android:fromYScale="0.9" Android:toYScale="1.0" Android:pivotX="50%p"
		Android:pivotY="50%p" Android:duration="250" />
	<!-- <alpha Android:fromAlpha="0.0" Android:toAlpha="1"
		Android:duration="400" /> -->
</set>  
